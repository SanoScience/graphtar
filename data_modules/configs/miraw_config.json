{
  "csv_path": "../../data/miRAW.csv",
  "batch_size": 128,
  "train_val_ratio": [
    0.8,
    0.2
  ],
  "x_key": "merged",
  "y_key": "label",
  "transform": [
    {
      "Pad": {
        "key_to_pad": "target",
        "target_length": 40,
        "padding_char": "N"
      }
    },
    {
      "Pad": {
        "key_to_pad": "mirna",
        "target_length": 30,
        "padding_char": "N"
      }
    },
    {
      "Merge": {
        "merged_key": "merged",
        "keys_to_merge": [
          "mirna",
          "target"
        ]
      }
    },
    {
      "ToOneHot": {
        "encoding_dict": {
          "A": 0,
          "U": 1,
          "T": 1,
          "G": 2,
          "C": 3,
          "N": 4
        },
        "key_to_encode": "merged"
      }
    },
    {
      "ToTensor": {
        "keys": [
          "merged",
          "label"
        ]
      }
    }
  ]
}